<?php

namespace DummyNameSpaceClass\Repositories\Backend\DummyPath\DummyClass;

use HalcyonLaravel\Base\Repository\BaseRepository as Repository;
use Illuminate\Database\Eloquent\Builder;
use DummyNameSpaceClass\Models\DummyPath\DummyClass\DummyClass as Model;

/**
* DummyClassRepository
*/
class DummyClassRepository extends Repository
{
    public function __construct(Model $model)
    {
        parent::__construct($model);
        $this->setObserver(new DummyClassObserver);
    }

    /**
     * @param Model $model
     *
     * @throws \Exception
     * @return $model
     */
    public function deleting($model)
    {
        $model->metable()->delete();
        return $model;
    }
}

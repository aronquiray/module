<?php

namespace Tests\Feature\Modules\Frontend\DummyPath\DummyClass;

use Tests\TestCase;
use Illuminate\Foundation\Testing\RefreshDatabase;
use DummyNameSpaceClass\Models\DummyPath\DummyClass\DummyClass as Model;

/**
 * Class DummyClassFeatureFrontendTest.
 */
class DummyClassFeatureFrontendTest extends TestCase
{
    use RefreshDatabase;

     public function setUp()
    {
        parent::setUp();
        $this->markTestSkipped('Skipped ' . __METHOD__);
        $this->loginAsAdmin(false);
        (new \DummyClassPermissionTableSeeder)->run();
        (new \DummyClassTableSeeder)->run();
        $this->prepareMenu(false);
    }

    public function testDummyClassRouteFrontShowExist()
    {        
        $model = Model::first();
        $this->get(route(Model::ROUTE_FRONTEND_PATH . '.show', $model))
            ->assertStatus(200); 
    }

    public function testDummyClassRouteFrontIndexExist()
    {        
        $this->get(route(Model::ROUTE_FRONTEND_PATH . '.index'))
            ->assertStatus(200); 
    }
}
